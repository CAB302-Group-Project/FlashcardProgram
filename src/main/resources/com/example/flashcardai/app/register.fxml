        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label layoutX="123.0" layoutY="14.0" text="Email" />
    <TextField fx:id="textEmail" layoutX="19.0" layoutY="45.0" prefHeight="25.0" prefWidth="240.0" />
    <Label layoutX="113.0" layoutY="93.0" text="Password" />
    <PasswordField fx:id="textPassword" layoutX="19.0" layoutY="122.0" prefHeight="25.0" prefWidth="240.0" />
    <Label layoutX="90.0" layoutY="170.0" text="Confirm Password" />
    <PasswordField fx:id="textConfirmPassword" layoutX="19.0" layoutY="196.0" prefHeight="25.0" prefWidth="240.0" />
    <Button fx:id="btnSignup" layoutX="86.0" layoutY="242.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="Sign Up" />
    <Button fx:id="btnHome" layoutX="87.0" layoutY="276.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="Back to main" />
</Pane>
